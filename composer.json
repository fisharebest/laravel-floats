{
  "name": "fisharebest/laravel-floats",
  "description": "Floating point support for Laravel migrations",
  "keywords": [
    "laravel",
    "mysql",
    "migration",
    "float",
    "double"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Greg Roach",
      "email": "greg@subaqua.co.uk"
    }
  ],
  "require": {
    "illuminate/database": "5.0 - 9.0"
  },
  "require-dev": {
    "phpunit/phpunit": "4.8 - 8.5",
    "yoast/phpunit-polyfills": "*",
    "php-coveralls/php-coveralls": "*"
  },
  "autoload": {
    "psr-4": {
      "Fisharebest\\LaravelFloats\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Fisharebest\\LaravelFloats\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "aliases": {
        "Schema": "Fisharebest\\LaravelFloats\\Schema"
      }
    }
  }
}
